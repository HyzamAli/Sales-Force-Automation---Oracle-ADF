<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="sales-flow-contact">
    <default-activity>contactsList</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>ContactsBean</managed-bean-name>
      <managed-bean-class>view.ContactsBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__6">
      <managed-bean-name>NotesBean</managed-bean-name>
      <managed-bean-class>view.NotesBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__7">
      <managed-bean-name>TasksBean</managed-bean-name>
      <managed-bean-class>view.TasksBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="contactsList">
      <page>/ContactsList.jsff</page>
    </view>
    <method-call id="ExecuteWithParams">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <view id="contactDetailed">
      <page>/contactDetailed.jsff</page>
    </view>
    <method-call id="ExecuteWithParams1">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ExecuteWithParams2">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <view id="customerDetailed">
      <page>/CustomersDetailed.jsff</page>
    </view>
    <view id="OpDetailed">
      <page>/oppurtunityDetailed.jsff</page>
    </view>
    <control-flow-rule id="__2">
      <from-activity-id>contactsList</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>toDetailed</from-outcome>
        <to-activity-id>ExecuteWithParams</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>ExecuteWithParams</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>contactDetailed</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>ExecuteWithParams1</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>customerDetailed</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>ExecuteWithParams2</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>OpDetailed</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>contactDetailed</from-activity-id>
      <control-flow-case id="__14">
        <from-outcome>toCustomerDetailed</from-outcome>
        <to-activity-id>ExecuteWithParams1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__15">
        <from-outcome>toOpDetailed</from-outcome>
        <to-activity-id>ExecuteWithParams2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
