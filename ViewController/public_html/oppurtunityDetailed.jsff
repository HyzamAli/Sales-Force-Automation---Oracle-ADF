                <dvt:statusMeterGauge id="statusMeterGauge1"
                                      shortDesc="#{bindings.CustomerTasksVO7.hints.PercentageCompletion.tooltip}"
                                      value="#{row.bindings.PercentageCompletion.inputValue}" minimum="0" maximum="100">
                  <dvt:gaugeMetricLabel rendered="true" id="gml1" textType="percent"/>
                </dvt:statusMeterGauge>
              </af:column>
              <af:column headerText="#{bindings.CustomerTasksVO7.hints.DueDate.label}" id="c11">
