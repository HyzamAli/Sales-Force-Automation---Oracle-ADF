    <af:showDetailItem id="Notes" shortDesc="Drawer 2" icon="#{resource['images:attachment.png']}" text="Notes">
      <af:panelGroupLayout id="pgl1">
        <af:panelBox id="pb2" showDisclosure="false" showHeader="always" text="Notes">
          <f:facet name="toolbar"/>
          <af:panelCollection id="pc1">
            <f:facet name="menus"/>
            <f:facet name="toolbar"/>
            <f:facet name="statusbar"/>
            <af:table value="#{bindings.CustomerNotesVO7.collectionModel}" var="row"
                      rows="#{bindings.CustomerNotesVO7.rangeSize}"
                      emptyText="#{bindings.CustomerNotesVO7.viewable ? 'No data to display.' : 'Access Denied.'}"
                      rowBandingInterval="0" fetchSize="#{bindings.CustomerNotesVO7.rangeSize}" id="t2"
                      summary="oppurtunity nores list" columnStretching="last">
              <af:column headerText="#{bindings.CustomerNotesVO7.hints.NotesTitle.label}" id="c1" rowHeader="true">
                <af:outputText id="ot21" value="#{row.NotesTitle}"
                               shortDesc="#{bindings.CustomerNotesVO7.hints.NotesTitle.tooltip}"/>
              </af:column>
              <af:column headerText="#{bindings.CustomerNotesVO7.hints.NotesDescription.label}" id="c2">
                <af:outputText id="ot22" value="#{row.NotesDescription}"
                               shortDesc="#{bindings.CustomerNotesVO7.hints.NotesDescription.tooltip}"/>
              </af:column>
              <af:column headerText="#{bindings.CustomerNotesVO7.hints.ContactId.label}" id="c3">
                <af:selectOneChoice value="#{row.bindings.ContactId.inputValue}" label="#{row.bindings.ContactId.label}"
                                    required="#{bindings.CustomerNotesVO7.hints.ContactId.mandatory}"
                                    shortDesc="#{bindings.CustomerNotesVO7.hints.ContactId.tooltip}" id="soc3">
                  <f:selectItems value="#{row.bindings.ContactId.items}" id="si3"/>
                  <f:validator binding="#{row.bindings.ContactId.validator}"/>
                </af:selectOneChoice>
              </af:column>
              <af:column headerText="#{bindings.CustomerNotesVO7.hints.RepId.label}" id="c4">
                <af:selectOneChoice value="#{row.bindings.RepId.inputValue}" label="#{row.bindings.RepId.label}"
                                    required="#{bindings.CustomerNotesVO7.hints.RepId.mandatory}"
                                    shortDesc="#{bindings.CustomerNotesVO7.hints.RepId.tooltip}" id="soc10">
                  <f:selectItems value="#{row.bindings.RepId.items}" id="si24"/>
                  <f:validator binding="#{row.bindings.RepId.validator}"/>
