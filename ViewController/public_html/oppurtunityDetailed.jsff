                      width="140%">
              <af:column headerText="#{bindings.CustomerTasksVO7.hints.TaskDetails.label}" id="c6" rowHeader="true">
                <af:outputText value="#{row.TaskDetails}"
                               shortDesc="#{bindings.CustomerTasksVO7.hints.TaskDetails.tooltip}" id="ot1"/>
              </af:column>
              <af:column headerText="#{bindings.CustomerTasksVO7.hints.ContactId.label}" id="c7">
                <af:selectOneChoice value="#{row.bindings.ContactId.inputValue}" label="#{row.bindings.ContactId.label}"
                                    required="#{bindings.CustomerTasksVO7.hints.ContactId.mandatory}"
                                    shortDesc="#{bindings.CustomerTasksVO7.hints.ContactId.tooltip}" readOnly="true"
                                    id="soc11">
                  <f:selectItems value="#{row.bindings.ContactId.items}" id="si25"/>
                  <f:validator binding="#{row.bindings.ContactId.validator}"/>
                </af:selectOneChoice>
