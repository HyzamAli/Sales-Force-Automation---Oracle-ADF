                        shortDesc="#{bindings.ProductQuantity.hints.tooltip}" id="it10">
            <f:validator binding="#{bindings.ProductQuantity.validator}"/>
            <af:convertNumber groupingUsed="false" pattern="#{bindings.ProductQuantity.format}"/>
          </af:inputText>
