<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <f:view>
        <af:document title="ProductsPage.jspx" id="d1">
            <af:messages id="m1"/>
             <af:form id="f1">
                <af:pageTemplate viewId="/AdminTemplate.jspx" id="pt1">
                    <f:attribute name="PanelText" value="Manage Products"/>
                    <f:facet name="body">
                        <af:panelGroupLayout layout="vertical" id="pgl1" inlineStyle="background-color:White;">
                            <af:query id="qryId1" headerText="Search" disclosed="true"
                                      value="#{bindings.ProductsVOCriteriaQuery1.queryDescriptor}"
                                      model="#{bindings.ProductsVOCriteriaQuery1.queryModel}"
                                      queryListener="#{bindings.ProductsVOCriteriaQuery1.processQuery}"
                                      queryOperationListener="#{bindings.ProductsVOCriteriaQuery1.processQueryOperation}"
                                      />
                            <af:table value="#{bindings.ProductsVO12.collectionModel}" var="row"
                                      rows="#{bindings.ProductsVO12.rangeSize}"
                                      emptyText="#{bindings.ProductsVO12.viewable ? 'No data to display.' : 'Access Denied.'}"
                                      rowBandingInterval="0"
                                      selectedRowKeys="#{bindings.ProductsVO12.collectionModel.selectedRow}"
                                      selectionListener="#{bindings.ProductsVO12.collectionModel.makeCurrent}"
                                      rowSelection="single" fetchSize="#{bindings.ProductsVO12.rangeSize}" id="t3"
                                      partialTriggers="::b3 ::b4 ::b1">
                                <af:column headerText="#{bindings.ProductsVO12.hints.ProductName.label}" id="c1">
                                    <af:inputText value="#{row.bindings.ProductName.inputValue}"
                                                  label="#{bindings.ProductsVO12.hints.ProductName.label}"
                                                  required="#{bindings.ProductsVO12.hints.ProductName.mandatory}"
                                                  columns="#{bindings.ProductsVO12.hints.ProductName.displayWidth}"
                                                  maximumLength="#{bindings.ProductsVO12.hints.ProductName.precision}"
                                                  shortDesc="#{bindings.ProductsVO12.hints.ProductName.tooltip}"
                                                  id="it5">
                                        <f:validator binding="#{row.bindings.ProductName.validator}"/>
                                    </af:inputText>
                                </af:column>
                                <af:column headerText="#{bindings.ProductsVO12.hints.ProductDescription.label}" id="c2">
                                    <af:inputText value="#{row.bindings.ProductDescription.inputValue}"
                                                  label="#{bindings.ProductsVO12.hints.ProductDescription.label}"
                                                  required="#{bindings.ProductsVO12.hints.ProductDescription.mandatory}"
                                                  columns="#{bindings.ProductsVO12.hints.ProductDescription.displayWidth}"
                                                  maximumLength="#{bindings.ProductsVO12.hints.ProductDescription.precision}"
                                                  shortDesc="#{bindings.ProductsVO12.hints.ProductDescription.tooltip}"
                                                  id="it6">
                                        <f:validator binding="#{row.bindings.ProductDescription.validator}"/>
                                    </af:inputText>
                                </af:column>
                                <af:column headerText="#{bindings.ProductsVO12.hints.ProductCategory.label}" id="c3">
                                    <af:inputText value="#{row.bindings.ProductCategory.inputValue}"
                                                  label="#{bindings.ProductsVO12.hints.ProductCategory.label}"
                                                  required="#{bindings.ProductsVO12.hints.ProductCategory.mandatory}"
                                                  columns="#{bindings.ProductsVO12.hints.ProductCategory.displayWidth}"
                                                  maximumLength="#{bindings.ProductsVO12.hints.ProductCategory.precision}"
                                                  shortDesc="#{bindings.ProductsVO12.hints.ProductCategory.tooltip}"
                                                  id="it7">
                                        <f:validator binding="#{row.bindings.ProductCategory.validator}"/>
                                    </af:inputText>
                                </af:column>
                                <af:column headerText="#{bindings.ProductsVO12.hints.ProductPrice.label}" id="c4">
                                    <af:inputText value="#{row.bindings.ProductPrice.inputValue}"
                                                  label="#{bindings.ProductsVO12.hints.ProductPrice.label}"
                                                  required="#{bindings.ProductsVO12.hints.ProductPrice.mandatory}"
                                                  columns="#{bindings.ProductsVO12.hints.ProductPrice.displayWidth}"
                                                  maximumLength="#{bindings.ProductsVO12.hints.ProductPrice.precision}"
                                                  shortDesc="#{bindings.ProductsVO12.hints.ProductPrice.tooltip}"
                                                  id="it8">
                                        <f:validator binding="#{row.bindings.ProductPrice.validator}"/>
                                        <af:convertNumber groupingUsed="false"
                                                          pattern="#{bindings.ProductsVO12.hints.ProductPrice.format}"/>
                                    </af:inputText>
                                </af:column>
                            </af:table>
                            <af:popup childCreation="deferred" autoCancel="disabled" id="p1">
                                <af:dialog id="d2" type="none" inlineStyle="background-color:White;">
                                    <f:facet name="buttonBar"/>
                                </af:dialog>
                                <af:panelFormLayout id="pfl1" inlineStyle="background-color:White;">
                                    <af:inputText value="#{bindings.ProductName.inputValue}"
                                                  label="#{bindings.ProductName.hints.label}"
                                                  required="#{bindings.ProductName.hints.mandatory}"
                                                  columns="#{bindings.ProductName.hints.displayWidth}"
                                                  maximumLength="#{bindings.ProductName.hints.precision}"
                                                  shortDesc="#{bindings.ProductName.hints.tooltip}" id="it1">
                                        <f:validator binding="#{bindings.ProductName.validator}"/>
                                    </af:inputText>
                                    <af:inputText value="#{bindings.ProductDescription.inputValue}"
                                                  label="#{bindings.ProductDescription.hints.label}"
                                                  required="#{bindings.ProductDescription.hints.mandatory}"
                                                  columns="#{bindings.ProductDescription.hints.displayWidth}"
                                                  maximumLength="#{bindings.ProductDescription.hints.precision}"
                                                  shortDesc="#{bindings.ProductDescription.hints.tooltip}" id="it2">
                                        <f:validator binding="#{bindings.ProductDescription.validator}"/>
                                    </af:inputText>
                                    <af:inputText value="#{bindings.ProductCategory.inputValue}"
                                                  label="#{bindings.ProductCategory.hints.label}"
                                                  required="#{bindings.ProductCategory.hints.mandatory}"
                                                  columns="#{bindings.ProductCategory.hints.displayWidth}"
                                                  maximumLength="#{bindings.ProductCategory.hints.precision}"
                                                  shortDesc="#{bindings.ProductCategory.hints.tooltip}" id="it3">
                                        <f:validator binding="#{bindings.ProductCategory.validator}"/>
                                    </af:inputText>
                                    <af:inputText value="#{bindings.ProductPrice.inputValue}"
                                                  label="#{bindings.ProductPrice.hints.label}"
                                                  required="#{bindings.ProductPrice.hints.mandatory}"
                                                  columns="#{bindings.ProductPrice.hints.displayWidth}"
                                                  maximumLength="#{bindings.ProductPrice.hints.precision}"
                                                  shortDesc="#{bindings.ProductPrice.hints.tooltip}" id="it4">
                                        <f:validator binding="#{bindings.ProductPrice.validator}"/>
                                        <af:convertNumber groupingUsed="false"
                                                          pattern="#{bindings.ProductPrice.format}"/>
                                    </af:inputText>
                                    <af:toolbar id="t2">
                                        <af:button actionListener="#{bindings.Create.execute}" text="Clear"
                                                   disabled="#{!bindings.Create.enabled}" id="b3"/>
                                        <af:button actionListener="#{bindings.Create.execute}" text="Add"
                                                   disabled="#{!bindings.Create.enabled}" id="b4"/>
                                    </af:toolbar>
                                </af:panelFormLayout>
                            </af:popup>
                            <af:toolbar id="t1">
                                <af:button text="Add" id="b2">
                                    <af:showPopupBehavior popupId="p1"/>
                                </af:button>
                                <af:button actionListener="#{bindings.Delete.execute}" text="Delete"
                                           disabled="#{!bindings.Delete.enabled}" id="b1"/>
                                <af:button actionListener="#{bindings.Commit.execute}" text="Save"
                                           disabled="#{!bindings.Commit.enabled}" id="b5"/>
                                <af:button actionListener="#{bindings.Rollback.execute}" text="Cancel"
                                           disabled="#{!bindings.Rollback.enabled}" immediate="true" id="b6">
                                    <af:resetActionListener/>
                                </af:button>
                            </af:toolbar>
                        </af:panelGroupLayout>
                    </f:facet>
                </af:pageTemplate>
            </af:form>
        </af:document>
    </f:view>
</jsp:root>
